#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules/.pnpm/rc@1.2.8/node_modules:/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules/.pnpm/node_modules:/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules:/Users/williamssissoko/WebstormProjects/node_modules:/Users/williamssissoko/node_modules:/Users/node_modules:/node_modules:/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules/.pnpm/registry-url@5.1.0/node_modules/registry-url/node_modules:/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules/.pnpm/registry-url@5.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules/.pnpm/rc@1.2.8/node_modules:/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules/.pnpm/node_modules:/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules:/Users/williamssissoko/WebstormProjects/node_modules:/Users/williamssissoko/node_modules:/Users/node_modules:/node_modules:/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules/.pnpm/registry-url@5.1.0/node_modules/registry-url/node_modules:/Users/williamssissoko/WebstormProjects/Cst_3145_Coursework_2/node_modules/.pnpm/registry-url@5.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
else
  exec node  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
fi
